<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.meongnyang.shop.repository.AdminMapper">

    <select id="findByUsername" resultType="com.meongnyang.shop.entity.admin.Admin">
        select
            id,
            admin_name,
            admin_password
        from
            admin_tb
        where
            admin_name = #{adminName}
    </select>
</mapper>
